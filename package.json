{
  "name": "public-chain-configs",
  "version": "0.0.1",
  "description": "Axelar Network public chain & assets registry",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "wizard": "ts-node-esm ./cli/wizard",
    "codegen:schemas": "bun run scripts/codegen-schemas.ts",
    "codegen:chains": "bun run scripts/codegen-chains.ts",
    "test": "vitest --run"
  },
  "keywords": [],
  "license": "LicenseRef-LICENSE",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "bun run test && bun run scripts/codegen-chains.ts && git add . && git commit -m 'chore: update generated files'"
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@types/prettier": "^2.7.3",
    "bun-types": "^0.7.3",
    "chalk": "^5.3.0",
    "husky": "^8.0.3",
    "jsonschema": "^1.4.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.4",
    "zx": "^7.2.3"
  }
}
